    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jarvis/shared": "workspace:*",
    "playwright-core": "^1.50.0",
    "ssh2": "^1.17.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/ssh2": "^1.15.5",
    "tsdown": "^0.9.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
